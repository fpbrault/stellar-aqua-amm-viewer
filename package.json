    "urijs": "^1.19.7"
  },
  "devDependencies": {
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-table": "^7.7.9",
    "@types/urijs": "^1.19.17",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "autoprefixer": "10.4.0",
    "eslint": "8.4.1",
